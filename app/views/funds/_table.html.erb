<!-- show stripped table -> width:6 and from_left:3 -->
<div class="col-md-6 col-md-offset-3">
  <table class="table table-striped" id="daily-repot-table">
    <thead>
      <tr>
        <th>Date</th>
        <th>Value</th>
      </tr>
    </thead>
    <tbody>
      <% @daily_reports.each do |daily_report| %>
          <tr>
            <td><%= daily_report[0] %></td>
            <td><%= daily_report[1] %></td>
          </tr>
      <% end %>
    </tbody>
  </table>
</div>