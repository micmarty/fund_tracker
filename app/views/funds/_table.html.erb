<!-- show stripped table -> width:6 and from_left:3 -->
<div class="col-md-6 col-md-offset-3">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Data</th>
        <th>Wartość</th>
      </tr>
    </thead>
    <tbody>
      <% @daily_reports.find_each do |daily_report| %>
          <tr>
            <td><%= daily_report.report_date %></td>
            <td><%= daily_report.value %></td>
          </tr>
      <% end %>
    </tbody>
  </table>
</div>